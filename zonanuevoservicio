$ORIGIN minuevoservicio.com.
$TTL 3H
@       IN SOA  server.minuevoservicio.com. root@minuevoservicio.com. (
                                        0       ; serial
                                        1D      ; refresh
                                        1H      ; retry
                                        1W      ; expire
                                        3H )    ; minimum
@       IN      NS      server.minuevoservicio.com.
;host en la zona

server  IN      NS      192.168.50.5
www     IN      CNAME   server
